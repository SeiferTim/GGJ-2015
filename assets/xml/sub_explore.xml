<?xml version="1.0" encoding="utf-8" ?>
<data>
<include id="_ui_globals"/>

	
	<group id="display" />

	
	<button_toggle id="button_0" x="-20" y="-20" label="Move 0" use_def="game_button" width="200" group="display">
		<anchor x="center" x-flush="right" y="center" y-flush="bottom" />
		<param type="string" value="button_0" />
	</button_toggle>
	<button_toggle id="button_1" x="20" y="-20" label="Move 1" use_def="game_button" width="200" group="display">
		<anchor x="center" x-flush="left" y="center" y-flush="bottom" />
		<param type="string" value="button_1" />
	</button_toggle>
	<button_toggle id="button_sp" x="-20" y="20" label="Move x" use_def="game_button" width="200" group="display">
		<anchor x="center" x-flush="right" y="center" y-flush="top" />
		<param type="string" value="button_sp" />
	</button_toggle>
	<button_toggle id="button_2sp" x="20" y="20" label="Move 2x" use_def="game_button" width="200" group="display">
		<anchor x="center" x-flush="left" y="center" y-flush="top" />
		<param type="string" value="button_2sp" />
	</button_toggle>
	<button id="button_cancel" x="0" y="-20" label="Cancel" use_def="game_button" width="200" group="display">
		<anchor x="center" x-flush="center" y="bottom" y-flush="bottom" />
		<param type="string" value="button_cancel" />
	</button>

	
</data>